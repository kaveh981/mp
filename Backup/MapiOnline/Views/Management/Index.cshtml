<body id="signin">
    <div class="container">
        <div id="heading">
            <div class="inner">
                <div id="logo" class="left">
                    <a href="http://www.mapiha.com">
                        <img style="width: 65px; height: 65px;" src="../../Content/Default/Images/Mapi-logo.png" /></a></div>
                <div class="clearer">
                </div>
            </div>
        </div>
        <div dir="rtl" class="content-inner">
            <div id="container">
                <div id="main-body">
                    <div class="content-inner">
                        <div class="size2of3">
                            <div class="form-no-labels friendly" id="signin-form">
                                <div class="section">
                                    <img src="../../Content/Default/Images/mapi-login.png" />
                                    <div class="input-row relative">
                                        <input value="m_zhoobin51" type="text" id="username" autocapitalize="off" placeholder="نام کاربری"
                                            autocorrect="off">
                                    </div>
                                    <div class="clearer">
                                    </div>
                                    <div class="input-row">
                                        <input type="password" value="123456" id="password" autocapitalize="off" autocomplete="off"
                                            placeholder="کلمه عبور" autocorrect="off">
                                        <a href="#" id="forgotPass" class="block strong forgot">فراموش شده؟</a>
                                        <div class="clearer">
                                        </div>
                                    </div>
                                    <div class="input-row holder">
                                        <div class="buttons frog">
                                            <button value="1" id="btn_Submit" style="width: 100px;">
                                                ورود</button>
                                            <div class="clearer">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-no-labels friendly invisible" id="forgot-form">
                                <h1 class="text-center">
                                    ثبت کلمه عبور جدید</h1>
                                <div class="section">
                                    <p class="text-center reset_details grey-font">
                                        درصورتی که ایمیلی برای نام کاربری شما ثبت نشده باشد، کلمه عبور جدید به ایمیل مدیر
                                        فروشگاه ارسال خواهد شد.</p>
                                    <fieldset>
                                        <div class="input-row relative">
                                            <input type="text" id="forgot_password_user_name" autocapitalize="off" placeholder="username"
                                                autocorrect="off">
                                            <div class="buttons frog">
                                                <button style="width: 100px; height: 30px;" id="btnForgotPassword">
                                                    ارسال کلمه عبور</button>
                                            </div>
                                        </div>
                                        <div class="clearer">
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearer">
                    </div>
                </div>
            </div>
        </div>
        <div dir="rtl" id="dialogHelp" style="margin: 0 0 0 0" class="invisible">
            <script src="../../Scripts/tinymce/jquery.tinymce.js" type="text/javascript"></script>
            <div id="help">
            </div>
        </div>
        <span id="spinner" class="spinner">Loading...</span>
        <div id="divContainer">
            <div class=" fixed_elem" dir="rtl">
                <div class="hidden" id="userDefault">
                    <div class="userDefault">
                        <input type="text" id="txt_s_Date" class="selectMedium1 " style="height: 13px;" maxlength="10" />
                        <select class=" selectMedium1" id="ddl_s_Branch">
                        </select>
                        <select disabled="disabled" class=" selectMedium1" id="ddl_m_Counter">
                            <option value="">انتخاب صندوق</option>
                        </select>
                        <select class=" selectMedium1" id="ddl_m_Currency">
                        </select>
                        <select class=" selectMedium1" id="ddl_m_Language">
                            <option value="pr">فارسی</option>
                            <option value="en">English</option>
                            <option value="fr">Français</option>
                        </select>
                    </div>
                    <div class=" menuUser">
                        <ul id="loginView" class="listH">
                            <li><a href='#' mllk="exit" id='an_m_SignOut'>exit</a> </li>
                            <li class='divider'>&nbsp;</li>
                            <li id='lbl_d_UserName'></li>
                            <li>
                                <div id="switcher">
                                </div>
                            </li>
                            <li class='divider'>&nbsp;</li>
                            <li><a mllk="help" href='#' id='a_help'>help</a> </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div id="mainTab" dir="rtl" class="mainbody">
            </div>
        </div>
        <div id="loginBox" class="orange
    blueHeader invisible" dir="rtl">
            <div>
                <label for="username" class="labelUp">
                    نام کاربری
                </label>
                <input size="18" id="username" value="" class=" inputText inputW136" placeholder="نام کاربری"
                    type="text" />
            </div>
            <div>
                <label for="password" class="labelUp">
                    رمز ورود
                </label>
                <div class="frog">
                    <input size="18" type="password" value="" id="password" class=" inputText inputW136"
                        placeholder="رمز ورود" type="text" /></div>
            </div>
        </div>
