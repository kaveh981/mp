@model IEnumerable< MapiOnline.Models.inv_Property>
           
           <!--/price panel end--> 
        
        <div class="panel panel-default">
          <div class="panel-heading">
            <h4 class="panel-title"> <a data-toggle="collapse"  href="#collapseBrand" class="collapseWill"> Brand <span class="pull-left"> <i class="fa fa-caret-right"></i></span> </a> </h4>
          </div>
          <div id="collapseBrand" class="panel-collapse collapse in">
            <div class="panel-body smoothscroll maxheight300">
            @foreach (var item in Model)
            {
                <div class="block-element">
                <label>
                  <input value="@item.PropertyId" type="checkbox" name="brands" value="0"  />
                  @item.Property </label>
              </div>
            }
            </div>
          </div>
        </div> 
        <!--/brand panel end-->

